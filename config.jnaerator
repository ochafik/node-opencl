-M__APPLE__=
//-DGLAPI=
//-DAPIENTRY=

-v

-mode Directory 
-f 
-d $(DIR)/src
-runtime NodeJS 

-framework OpenGL 
-framework OpenCL 
  "-TIOSurfaceRef=void*"
